@model IEnumerable<SIGE.Models.Calificacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AsignacionFk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstudianteFk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nota)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCalificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observaciones)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AsignacionFk.CodAsignacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstudianteFk.Apellido1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCalificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observaciones)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodCalificacion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodCalificacion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodCalificacion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
